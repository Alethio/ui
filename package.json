    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "rimraf": "^2.6.2",
    "styled-components": "^3.4.2",
    "ts-loader": "^6.0.4",
    "tslib": "^1.9.3",
    "tslint": "^5.18.0",
    "ttypescript": "^1.5.7",
    "typescript": "^3.6.4",
